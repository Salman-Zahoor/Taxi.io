{
  "expo": {
    "name": "Tax.io",
    "slug": "Tax.io",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
           "apiKey": "https://maps.googleapis.com/maps/api/js?key=AIzaSyAGsI054v7tVM-sT-U4Tc4bfnCe-c1kxTw_KEY&callback=initMap"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
